@CHARSET "UTF-8";
.sinput{
	font-size: 18px;
	border:none;
	border-bottom: 2px solid black;
	text-align: center;
	color: red;
}

.stext{
	font-size: 24px;
	font-weight: bold;
}

.outBorder{
	border: 1px solid #ccc;
	padding: 40px;
	background-color: #eee;
	margin-top: 50px;
}

.Btn{
	font-size:16px;
	color:white;
	font-weight:bolder;
	font-family:"微软雅黑,黑体";
	
	padding-left:30px;
	padding-right: 30px;
	padding-top: 10px;
	padding-bottom: 10px; 
}
.start{
	background-color: #060;
}

.end{
	background-color: #f60;
}

.time{
	font-size:16px;
	color:#063;
	text-align: center;
	padding-top: 20px;
	font-weight:bolder;
	line-height: 2.5;
}

.passTime{
	color:red;
	font-size:16px;
}

.tb{
	margin: 20px auto;
	width: 700px;
}